{
    "ProfilerEnabled": true,
    "LogLevel": 1,
    "DefaultLogType": "fileLoggerDefault",
    "AppName" : "nemo-",
    "FormatType" : "json",
    "FileLogger": [
        {
            "Key": "fileLoggerDefault",
            "Path": "/var/log/nemo/",
            "FileNamePrefix": "api-"       
        },
        {
            "Key": "cacheError",
            "Path": "/var/log/nemo/",
            "FileNamePrefix": "api-cache-"            
        }        
    ]
}
